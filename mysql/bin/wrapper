#!/bin/sh
LD_LIBRARY_PATH=/home/css/edx-edutech/mysql/lib:/home/css/edx-edutech/mysql/../common/lib${LD_LIBRARY_PATH:+:$LD_LIBRARY_PATH}
export LD_LIBRARY_PATH
case "$@" in
  *--no-defaults*)
    exec $0.bin "$@"
    ;;
  *--defaults-extra-file*)
    exec $0.bin "$@"
    ;;
esac
exec $0.bin --defaults-file=/home/css/edx-edutech/mysql/my.cnf "$@"
