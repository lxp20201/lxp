#!/bin/sh
LD_LIBRARY_PATH="/home/css/edx-edutech/common/lib${LD_LIBRARY_PATH:+:$LD_LIBRARY_PATH}"
DYLD_FALLBACK_LIBRARY_PATH="/home/css/edx-edutech/common/lib${DYLD_FALLBACK_LIBRARY_PATH:+:$DYLD_FALLBACK_LIBRARY_PATH}"
CURL_CA_BUNDLE="/home/css/edx-edutech/common/openssl/certs/curl-ca-bundle.crt"
export LD_LIBRARY_PATH
export DYLD_FALLBACK_LIBRARY_PATH
export CURL_CA_BUNDLE
exec /home/css/edx-edutech/common/bin/curl.bin "$@"
